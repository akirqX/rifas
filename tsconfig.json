{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "baseUrl": "./",
      "strict": true,
      "skipLibCheck": true,
      "noImplicitAny": false,
      "paths": {
        "@Assets/*": ["resources/assets/*"],
        "@Components/*": ["resources/js/Components/*"],
        "@Composables/*": ["resources/js/Composables/*"],
      },
      "typeRoots": [
        "resources/js/Types/*.d.ts",
      ]
    },
    "include": [
        "resources/js/**/*.vue",
        "resources/js/**/*.ts",
    ]
  }
